FUNCTION sign_div_unsign_3nh (denominator[9..0], numerator[20..0])
RETURNS ( quotient[20..0], remainder[9..0]);

--synthesis_resources = lut 192 
SUBDESIGN lpm_divide_pfm
( 
	denom[9..0]	:	input;
	numer[20..0]	:	input;
	quotient[20..0]	:	output;
	remain[9..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_3nh;
	numer_tmp[20..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
