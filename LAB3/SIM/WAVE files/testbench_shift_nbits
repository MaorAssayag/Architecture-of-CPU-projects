onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /testbench_shift_nbits/A
add wave -noupdate /testbench_shift_nbits/dir
add wave -noupdate /testbench_shift_nbits/enable
add wave -noupdate /testbench_shift_nbits/Aout
TreeUpdate [SetDefaultTree]
WaveRestoreCursors {{Cursor 1} {315175 ps} 0}
quietly wave cursor active 1
configure wave -namecolwidth 150
configure wave -valuecolwidth 100
configure wave -justifyvalue left
